<% if logged_in? session['username'],session['token'] %>
<div id="<%= dom_id entry %>">
  <p>
    <strong>Name:</strong>
    <%= entry.name %>
  </p>

  <p>
    <strong>Time in:</strong>
    <%= entry.time_in %>
  </p>

  <p>
    <strong>Time out:</strong>
    <%= entry.time_out %>
  </p>

  <p>
    <strong>Mobile phone:</strong>
    <%= entry.mobile_phone %>
  </p>


</div>
  <% else%>
  <p>Not Logged In</p>
<% end %>
